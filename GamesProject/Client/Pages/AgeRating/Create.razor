@page "/AgeRating/create/"
@inject HttpClient _client
@inject NavigationManager _navManager

<h3 class="card-title">Create New AgeRating</h3>
<hr />
<br />

<FormComponent ButtonText="Create AgeRating" ageRating="ageRating" OnValidSubmit="@CreateAgeRating" />

@code {

    AgeRating ageRating = new AgeRating();

    private async Task CreateAgeRating()
    {
        await _client.PostAsJsonAsync(Endpoints.AgeRatingsEndpoint, ageRating);
        _navManager.NavigateTo("/AgeRating");
    }

}